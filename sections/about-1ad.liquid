     <script>
        const mian = document.querySelector('.mianpage').querySelector('img');
        const mobilesrc = "https://cdn.shopify.com/s/files/1/0529/8647/8778/files/Group_1000001783_1.png?v=1700070899";
        const desktopsrc = "https://cdn.shopify.com/s/files/1/0529/8647/8778/files/Group_1000001779.png?v=1699999720";
        

        function myFunction(x) {
            if (x.matches) { 
                mian.src = mobilesrc;
            } else {
                mian.src = desktopsrc;
            }
        }

        var x = window.matchMedia("(max-width: 600px)")
        myFunction(x) 
        x.addListener(myFunction) 


     </script>

