        function myFunction(x) {
            if (x.matches) { 
                mian.src = mobilesrc;
            } else {
                mian.src = desktopsrc;
            }
        }

        var x = window.matchMedia("(max-width: 600px)")
        myFunction(x) 
        x.addListener(myFunction) 


